{
  "name": "IndexedDB My Admin",
  "version": "1.0",
  "description": "",
  "permissions": ["activeTab", "declarativeContent", "tabs", "<all_urls>"],
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"],
      "run_at": "document_end",
      "all_frames": false
    }
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icons": {
      "16": "images/app/get_started16.png",
      "32": "images/app/get_started32.png",
      "48": "images/app/get_started48.png",
      "128": "images/app/get_started128.png"
    }
  },
  "icons": {
    "16": "images/app/get_started16.png",
    "32": "images/app/get_started32.png",
    "48": "images/app/get_started48.png",
    "128": "images/app/get_started128.png"
  },
  "manifest_version": 2
}
