{
  "name": "indexeddb-my-admin-extension",
  "version": "0.0.1",
  "description": "IndexedDB My Admin Extension",
  "main": "index.js",
  "scripts": {
    "test": "jest --config jest/jest.conf.js --detectOpenHandles; exit 0",
    "test:coverage": "jest --config jest/jest.conf.js --coverage --detectOpenHandles; exit 0",
    "test:watch": "jest --config jest/jest.conf.js --watch --detectOpenHandles; exit 0",
    "build:extension": "webpack --config build/extension.js --progress",
    "watch:extension": "webpack --config build/extension.js --progress --watch",
    "build:browser": "webpack-dev-server --config build/browser.js --progress",
    "watch:browser": "webpack-dev-server --config build/browser.js --progress --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/duduindo/indexeddb-my-admin-extension.git"
  },
  "keywords": [
    "extension",
    "indexeddb",
    "chrome",
    "firefox"
  ],
  "author": "Eduardo <duduindo@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://gitlab.com/duduindo/indexeddb-my-admin-extension/issues"
  },
  "homepage": "https://gitlab.com/duduindo/indexeddb-my-admin-extension#readme",
  "dependencies": {},
  "devDependencies": {
    "@types/chrome": "0.0.91",
    "@types/jest": "^24.0.20",
    "@types/mocha": "^5.2.7",
    "copy-webpack-plugin": "^5.0.4",
    "dotenv": "^8.2.0",
    "stylus": "^0.54.7",
    "svelte": "^3.12.1",
    "svelte-loader": "^2.13.6",
    "svelte-preprocess": "^3.2.0-alpha.2",
    "ts-loader": "^6.2.1",
    "typescript": "^3.6.4",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  }
}
